{
    "name": "magellan",
    "description": "The best recipes on the web, all in one place",
    "authors": "Dan Duggan",
    "license": "MIT",
    "version": "1.1.0",
    "private": true,
    "scripts": {
        "start": "npm run prod",
        "server": "nodemon server.js",
        "magellan": "npm start --prefix magellan",
        "build": "react-scripts build",
        "live": "serve -l 3000 -s build",
        "dev": "concurrently \"npm run server\" \"npm run magellan\"",
        "prod": "concurrently \"npm run server\" \"npm run live\"",
        "index_database": "node database/dbIndexing.js",
        "populate_database": "node database/populateDB.js",
        "rebuild_database": "npm run index_database && npm run populate_database"
    },
    "dependencies": {
        "app-root-path": "^3.0.0",
        "concurrently": "^5.1.0",
        "dotenv": "^8.2.0",
        "express": "4.17.1",
        "nodemon": "^2.0.3",
        "react": "^16.13.1",
        "react-dom": "16.13.1",
        "react-helmet": "^6.0.0",
        "react-router-dom": "^5.1.2",
        "react-spinners": "^0.8.1"
    },
    "devDependencies": {
        "react-scripts": "^3.4.1"
    },
    "proxy": "http://localhost:5000"
}
